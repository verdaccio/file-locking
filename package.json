{
  "name": "@verdaccio/file-locking",
  "version": "1.0.0",
  "description": "library that handle file locking",
  "main": "lib/index.js",
  "keywords": [
    "lock",
    "fs"
  ],
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@verdaccio/types": "6.1.0",
    "@verdaccio/babel-preset": "0.1.0",
    "codecov": "3.5.0",
    "eslint": "5.16.0",
    "eslint-config-google": "0.13.0",
    "eslint-plugin-flowtype": "3.9.1",
    "flow-bin": "0.82.0",
    "husky": "0.14.3",
    "in-publish": "2.0.0",
    "jest": "24.8.0",
    "lint-staged": "8.1.7"
  },
  "author": "Juan Picado <juanpicado19@gmail.com>",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "lockfile": "1.0.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "*.{js, json}": [
      "npm run lint",
      "git add"
    ]
  },
  "homepage": "https://github.com/verdaccio/file-locking",
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "coverage": "codecov",
    "test": "jest",
    "lint": "npm run flow && npm run js-lint",
    "js-lint": "eslint .",
    "flow": "flow",
    "build": "babel src/ --out-dir lib/ --copy-files --ignore __tests__",
    "precommit": "lint-staged",
    "prepublish": "in-publish && npm run lint && npm run build || not-in-publish"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=4.6.1",
    "npm": ">=2.15.9"
  }
}
